{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": [
      "esnext"
    ],
    "strict": true,
    "rootDir": "",
    "plugins": [
      {
        "name": "nexus/typescript-language-service"
      }
    ],
    "typeRoots": [
      "node_modules/@types",
      "types"
    ],
    "esModuleInterop": true,
    "outDir": "dist"
  },
  "include": [
    "app.ts",
    "types.d.ts"
  ],
}